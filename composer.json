{
    "name": "bymayo/porter",
    "description": "A toolkit with lots of helpers for users and accounts",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "porter"
    ],
    "support": {
        "docs": "https://github.com/bymayo/porter/blob/craft-4/README.md",
        "issues": "https://github.com/bymayo/porter/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Mayo",
            "homepage": "https://bymayo.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^3.0",
        "craftcms/rector": "dev-main"
    },
    "autoload": {
        "psr-4": {
          "bymayo\\porter\\": "src/"
        }
    },
    "extra": {
        "name": "Porter",
        "handle": "porter",
        "developer": "Jason Mayo",
        "developerUrl": "https://bymayo.co.uk",
        "documentationUrl": "https://github.com/bymayo/porter/blob/craft-4/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/bymayo/porter/craft-4/CHANGELOG.md",
        "components": {
            "porterService": "bymayo\\porter\\services\\PorterService"
        },
        "class": "bymayo\\porter\\Porter"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
